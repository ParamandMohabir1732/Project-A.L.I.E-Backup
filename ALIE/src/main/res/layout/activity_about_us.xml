<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/primary"
    tools:context=".AboutUsActivity">

    <ImageButton
        android:id="@+id/TheSecondDawnImageButton14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:background="@null"
        android:contentDescription="@string/imageButton14"
        android:src="@drawable/ic_left_arrow_state" />

    <ImageView
        android:id="@+id/TheSecondDawnImageView49"
        android:layout_width="325dp"
        android:layout_height="150dp"
        android:layout_gravity="center"
        android:layout_marginTop="-15dp"

        app:srcCompat="@drawable/about_us_image" />

    <TextView
        android:id="@+id/TheSecondDawnTextView43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:text="@string/textview24"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="282dp" />

    <TextView
        android:id="@+id/TheSecondDawnTextView44"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="10dp"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:textSize="18dp"
        android:text="@string/textview25" />

    <ImageView
        android:id="@+id/TheSecondDawnImageView50"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="10dp"
        app:srcCompat="@drawable/about_us_1" />

</LinearLayout>