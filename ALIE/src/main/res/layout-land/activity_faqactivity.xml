<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/primary"
    tools:context=".FAQActivity">

    <ImageButton
        android:id="@+id/TheSecondDawnImageButton12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:background="@null"
        android:contentDescription="@string/imageButton12"
        android:src="@drawable/ic_left_arrow_state" />

    <ImageView
        android:id="@+id/TheSecondDawnImageView51"
        android:layout_width="290dp"
        android:layout_height="129dp"
        android:layout_gravity="center"
        android:layout_marginTop="-55dp"
        app:srcCompat="@drawable/faq_image" />

    <TextView
        android:id="@+id/TheSecondDawnTextView45"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:layout_marginTop="-55dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:text="@string/textview17" />

    <TextView
        android:id="@+id/TheSecondDawnTextView46"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:text="@string/textview18"
        android:textColor="@color/white"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/TheSecondDawnTextView47"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:text="@string/textview19" />

    <TextView
        android:id="@+id/TheSecondDawnTextView48"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/textview20"
        android:layout_marginTop="0dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:textColor="@color/white"
        android:textSize="15dp"/>

    <TextView
        android:id="@+id/TheSecondDawnTextView49"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:textSize="20dp"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:text="@string/textview21" />

    <TextView
        android:id="@+id/TheSecondDawnTextView50"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/textview22"
        android:layout_marginTop="0dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:textColor="@color/white"
        android:textSize="15dp"/>

    <TextView
        android:id="@+id/TheSecondDawnTextView51"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:text="@string/textview23" />

    <ImageView
        android:id="@+id/TheSecondDawnImageView52"
        android:layout_width="79dp"
        android:layout_height="55dp"
        android:layout_gravity="center"
        android:layout_marginTop="-20dp"
        app:srcCompat="@drawable/email_us" />

    <ImageView
        android:id="@+id/TheSecondDawnImageView53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:tint="@color/white"
        android:layout_marginStart="130dp"
        android:layout_marginTop="-40dp"
        app:srcCompat="@drawable/ic_arrow_forward" />


</LinearLayout>